{
    "title": "go 学习笔记",
    "author": "张德满 <mail : go_developer@163.com phone : 17710580607>",
    "description": "auto-api服务设计使用文档",
    "language": "zh-hans",
    "links" : {
        "sidebar" : {
            "Github地址" : "https://github.com/go-developer"
        }
    },
    "plugins": [
        "-search",
        "search-pro",
        "advanced-emoji",
        "github",
        "splitter",
        "-tbfed-pagefooter",
        "toggle-chapters",
        "sectionx",
        "codeblock-filename",
        "donate",
        "back-to-top-button",
        "-expandable-chapters",
        "chapter-fold",
        "code",
        "todo",
        "page-copyright",
        "-page-treeview",
        "simple-page-toc",
        "page-toc-button",
        "flexible-alerts",
        "auto-scroll-table",
        "pageview-count",
        "popup",
        "-theme-fexa",
        "theme-cornerstone",
        "-theme-gestalt",
        "hide-element",
        "insert-logo",
        "baidu-tongji",
        "-expandable-chapters-small-auto",
        "-book-summary-scroll-position-saver",
        "ad",
        "mermaid-flow",
        "-include-html",
        "include"
    ],
    "pluginsConfig": {
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord" : ["Gitbook Use"]
        },
        "github": {
            "url": "https://github.com/go-developer/study-note.git"
        },
        "sharing": {
            "weibo": true,
            "facebook": false,
            "twitter": false,
            "google": false,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper"
            ]
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy 张德满<go_developer@163.com> 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "donate": {
            "wechat": "/img/WechatIMG90.jpeg",
            "title": "感谢赞助",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "page-copyright": {
            "description": "文档修改于",
            "signature": "张德满",
            "wisdom": "开发者 & 文档维护者 张德满",
            "format": "YYYY-MM-dd hh:mm:ss",
            "copyright": "版权所有 &#169; 张德满",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "style": "normal",
            "noPowered": true,
            "baseUri": "https://github.com/go-developer",
            "repo": "study-note"
        },
        "page-toc-button": {
            "maxTocDepth": 5,
            "minTocSize": 5
        },
        "flexible-alerts": {
            "note": {
                "label": {
                  "de": "Hinweis",
                  "en": "Note"
                }
            },
            "tip": {
                "label": {
                  "de": "Tipp",
                  "en": "Tip"
                }
            },
            "warning": {
                "label": {
                  "de": "Warnung",
                  "en": "Warning"
                }
            },
            "danger": {
                "label": {
                  "de": "Achtung",
                  "en": "Attention"
                }
            }
        },
        "hide-element": {
            "elements": ["gitbook-link"]
        },
        "insert-logo": {
            "url": "/img/head-logo.jpeg",
            "style": "background: none; max-height: 80px; min-height: 80px"
        },
        "baidu-tongji": {
            "token": "5e7291930ee4174ab0c82a9a93cdc935"
        },
        "ad": {
          "contentTop": "",
          "contentBottom": ""
        }
    },
    "variables": {
        "themeFexa":{
            "nav":[
                {
                    "url":"/",
                    "target":"_blank",
                    "name": "张德满学习笔记"
                }
            ]
        }
    }
}
